SELECT          TOP (200) command_name, command_result_index, target, region, column_name, column_content, row_index, 
                            col_index, rowspan, colspan
FROM              PYPortalCommandScale
WHERE          (command_name = 'GetIDLSalFormPersonalBudget') AND (target = 'Default')
ORDER BY   command_name, command_result_index, target, region DESC, row_index, col_index


