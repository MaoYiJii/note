    語系切換
    
    -- AUS
    salarySAPProxy.Connection.Open();
    string E_Run_Msg = string.Empty;
    ZIPY001Table T_Account = new ZIPY001Table();
    ZIPY002Table T_Item = new ZIPY002Table();
    ZIPY003Table T_Absovt = new ZIPY003Table();
    ZIPY004Table T_Quota = new ZIPY004Table();
    ZIPY005Table T_Other = new ZIPY005Table();
    salarySAPProxy.Zhrpy_Getpayrollresult_AUS(startDate.ToString(), endDate.ToString(), queryType.ToString(), employeeNO, languangeType.ToString(), out E_Run_Msg, ref T_Absovt, ref T_Account, ref T_Item, ref T_Quota, ref T_Other);
    DataTable accountDataFromSAP = GetAccountDataFromSAP(T_Account);
    DataTable itemDataFromSAP = GetItemDataFromSAP(T_Item);
    DataTable absovtDataFromSAP = GetAbsovtDataFromSAP(T_Absovt);
    DataTable quotaDataFromSAP = GetQuotaDataFromSAP(T_Quota);
    DataTable otherDataFromSAP = GetOtherDataFromSAP(T_Other);
    dataSet = new DataSet();
    dataSet.Tables.Add(accountDataFromSAP.Copy());
    dataSet.Tables.Add(itemDataFromSAP.Copy());
    dataSet.Tables.Add(absovtDataFromSAP.Copy());
    dataSet.Tables.Add(quotaDataFromSAP.Copy());
    dataSet.Tables.Add(otherDataFromSAP.Copy());
    return dataSet;
    
    -- 一般
    salarySAPProxy.Connection.Open();
    string E_Run_Msg = string.Empty;
    ZIPY001Table T_Account = new ZIPY001Table();
    ZIPY002Table T_Item = new ZIPY002Table();
    ZIPY003Table T_Absovt = new ZIPY003Table();
    ZIPY004Table T_Quota = new ZIPY004Table();
    salarySAPProxy.Zhrpy_Getpayrollresult(startDate.ToString(), endDate.ToString(), queryType.ToString(), employeeNO, languangeType.ToString(), out E_Run_Msg, ref T_Absovt, ref T_Account, ref T_Item, ref T_Quota);
    DataTable accountDataFromSAP = GetAccountDataFromSAP(T_Account);
    DataTable itemDataFromSAP = GetItemDataFromSAP(T_Item);
    DataTable absovtDataFromSAP = GetAbsovtDataFromSAP(T_Absovt);
    DataTable quotaDataFromSAP = GetQuotaDataFromSAP(T_Quota);
    dataSet = new DataSet();
    dataSet.Tables.Add(accountDataFromSAP.Copy());
    dataSet.Tables.Add(itemDataFromSAP.Copy());
    dataSet.Tables.Add(absovtDataFromSAP.Copy());
    dataSet.Tables.Add(quotaDataFromSAP.Copy());
    return dataSet;
    
