3/26
    // 來文單位
    select distinct out_org from rps_main where out_org like '%" + keyWord + "%'
    // 所屬廠區
    select profile_code, code_value from Profile_c where active='Y' and profile_type=@profile_type -- @profile_type = 1
    // 受文者
    select profile_code, code_value from Profile_c where active='Y' and profile_type=@profile_type -- @profile_type = 2
    // 收文者
    select RTRIM(eng_name) as eng_name, emp_name, logonid, emp_no, org_id, ext_no, e_mail from emp_data where active='Y' and (emp_no + ';' + emp_name + ';' +eng_name+ ';' +org_id) like '%" + keyWord + "%'
    
// 密件 收文者
-- @role_id = 3
select distinct emp_no from RoleAuthSite_r a inner join Role_r b 
 on a.role_id=b.role_id 
 where a.role_id=@role_id and a.active='Y' and b.active='Y' and a.site like @site
 // 從 emp_no 帶出完整資料
 SELECT org_id, ext_no, e_mail,RTRIM(eng_name) as eng_name,emp_no, emp_name, site, sex, logonid FROM emp_data WHERE emp_no = @emp_no AND active = 'Y'
    
