離職人員語言成績查詢(for HR)	            2

        Dim RoleList As ArrayList = bpl.getUserRoleName(Session("str_cap_emp_no"))
        For Each item As String In RoleList
            If item.Trim = "Super_User" OrElse item.Trim = "Super_Mgr" OrElse item.Trim = "C&B_Mgr" Then
                Me.role_mode = clsBPL.mode_type.CorpHR
                Exit For
            End If
        Next

        If Me.role_mode <> clsBPL.mode_type.CorpHR Then
            If bpl.CheckBossUnder30(Session("str_cap_emp_no")) Then
                role_mode = clsBPL.mode_type.boss

                If Me.agvEmpData.Columns.Count >= 15 Then
                    Me.agvEmpData.Columns(14).Visible = True
                End If

            ElseIf bpl.CheckIsSecretary(Session("str_cap_emp_no")) Then
                role_mode = clsBPL.mode_type.secretary
            Else
                role_mode = clsBPL.mode_type.others
            End If
        End If
        
