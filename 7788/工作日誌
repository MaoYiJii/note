    撤回
        Read_Auth reader = new Read_Auth();
        reader.rps_no = rps_no;
        reader.app_date = DateTime.Now;
        reader.auth_date = DateTime.Now;
        reader.reason = tbRollBack.Text.Trim();
        reader.action = "撤回";
        reader.auth_to = Request.Cookies["logonid"].Value;
        model.AddToRead_Auth(reader);

        Rps_main main = model.rps_main.First(i => i.rps_no == rps_no);
        main.sta = "3";
    重新指派
        Read_Auth new_reader = new Read_Auth();
        new_reader.rps_no = rps_no;
        new_reader.auth_to = tempInfo[0];
        new_reader.auth_date = DateTime.Now;
        model.AddToRead_Auth(new_reader);

        //修改表單狀態
        Rps_main main = model.rps_main.First(i => i.rps_no == rps_no);
        main.sta = "0";

        //修改當前收件者
        Pass_main pm = model.pass_main.First(i => i.scc_no == rps_no);
        pm.receiver = tempInfo[0];
        
    檔案下載
        string pageUrl = WebConfigurationManager.AppSettings["fileGet"];
        //文件獲取頁面地址(測試區)
        string sysID = WebConfigurationManager.AppSettings["sysID"];        //系統ID[必選]，新系統申請時固定分配
        string bizID = rps_no;        //業務ID[可選]，由AP系統生成，用於管理一組文件，可根據其獲取文件List
        string action = WebConfigurationManager.AppSettings["action"];      //操作類型Action[必選]，VDTMPF的字符組合
        string empNo = Request.Cookies["emp_no"].Value;                     //操作者工號[必選]
        string language = WebConfigurationManager.AppSettings["language"];  //頁面顯示語言[必選]，en / zh-CHS / zh-CHT

        string orignQueryString = "sys_id=" + sysID + "&biz_id=" + bizID + "&action=" + action + "&emp_no=" + empNo + "&language=" + language;
        string url = pageUrl + SecurityHelper.Encrypt(orignQueryString, true);
        
