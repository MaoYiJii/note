測試 FlowER
        [HttpGet, Route("TestCreateNewForm")]
        public IHttpActionResult TestCreateNewForm(string formKind)
        {
            var result = _managerService.TestCreateNewForm(formKind);
            return Json(result);
        }

        public FlowerCreateNewFormResponse TestCreateNewForm(string formKind)
        {
            // AUO.FORM.466
            HttpWebRequest request = (HttpWebRequest)WebRequest.Create($"http://auhqitapts1.corpnet.auo.com/FlowER20PublicAPI/Form/CreateNewForm?formKind={formKind}&filler={LoginData.EmpNo}");
            request.Method = "GET";
            request.Headers.Add("formKind", formKind);
            request.Headers.Add("securityCode", "{F032C8B4-9239-49BA-AC94-D237E31F1D00}");
            request.ContentType = "application/json";
            using (var responseStream = request.GetResponse().GetResponseStream())
            {
                using (var responseReader = new StreamReader(responseStream, Encoding.UTF8))
                {
                    var responseJson = responseReader.ReadToEnd();
                    return JsonConvert.DeserializeObject<FlowerCreateNewFormResponse>(responseJson);
                }
            }
        }



