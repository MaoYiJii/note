UPDATE Platform_ParaSetting_C
SET    [data_source] = '
    SELECT '''' AS code_name
    UNION ALL
    SELECT code_name
    FROM   AccessControlCodeMap
    WHERE  code_type = ''Site''
           AND active = ''Y''
    ORDER  BY code_name '
WHERE  para_name = 'site' 



