4/1
    Query Normal
        select a.rps_no, a.in_no, a.create_by, b.pass_no, b.receiver, a.out_org, b.s_date, convert(nvarchar,a.out_date,111) as 'out_date'
        from dbo.rps_main a inner join dbo.pass_main b on a.rps_no=b.scc_no
        inner join Read_Auth c on a.rps_no=c.rps_no 
        where 1=1 
        and a.in_no=@in_no
        and a.sta=@sta
        and a.out_org=@out_org
        and a.out_no like '%" + out_no + "%'
        and b.s_date between @ReceiveDateFrom and @ReceiveDateTo
        and a.out_date between @deliverDateFrom and @deliverDateTo
        and a.receiver_site in ("+receiver_site+")
        and c.auth_to =@auth_to
        order by a.create_date desc
        
    Query Admin
        select a.rps_no,a.in_no, a.create_by, b.pass_no, b.receiver, a.out_org, convert(nvarchar,a.out_date,111) as 'out_date' 
        from dbo.rps_main a inner join dbo.pass_main b on a.rps_no=b.scc_no
        where 1=1
        
